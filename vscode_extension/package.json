{
  "name": "nhtml",
  "displayName": "nhtml",
  "description": "Syntax highlighting for .nhtml files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "nhtml",
      "aliases": ["nhtml", "nhtml"],
      "extensions": [".nhtml"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "nhtml",
      "scopeName": "source.nhtml",
      "path": "./syntaxes/nhtml.tmLanguage.json",
      "embeddedLanguages": {
        "meta.embedded.block.javascript": "javascript",
        "meta.embedded.block.css": "css"
      }
    }]
  }
}
